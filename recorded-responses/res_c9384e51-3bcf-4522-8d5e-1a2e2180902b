angular.module('sn.$sp').directive('snScAlsoRequestFor', function($http, spUtil, filterExpressionParser, escapeHtml, i18n, md5, spAriaUtil, select2EventBroker, spAriaFocusManager, spScUtil, spSCConf) {
'use strict';
return {
restrict: 'E',
replace: true,
scope: {
ed: "=?",
field: "=",
refTable: "=?",
