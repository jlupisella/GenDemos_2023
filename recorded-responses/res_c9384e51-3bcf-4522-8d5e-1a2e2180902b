;
/*! RESOURCE: /scripts/sn/common/clientScript/uiPolicyTypes.js */
(function(exports, undefined) {
'use strict';
var UI_POLICY_TYPES = {
dateTypes: {
glide_date_time: "datetime",
glide_date: "date",
date: "date",
datetime: "datetime",
due_date: "datetime"
},
numberTypes: {
decimal: "decimal",
numeric: "numeric",
integer: "integer",
float: "float"
},
