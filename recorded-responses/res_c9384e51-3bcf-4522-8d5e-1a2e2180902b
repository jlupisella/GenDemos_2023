if (child && child.tooltip)
child.tooltip({
'template':	'<div class="tooltip" style="white-space: pre-wrap" role="tooltip"><div class="tooltip-arrow"></div><div class="tooltip-inner"></div></div>',
'placement': 'top',
'container': 'body'
});
$scope.snComposingPresence = snComposingPresence;
}
}
