g_env: g_env,
getUserGlideForm: g_env.getUserGlideForm,
initialize: function() {
g_env.initScripts(clientScripts, validationScripts);
if (uiPolicies && (uiPolicies.length > 0)) {
g_env.initUIPolicyScripts(uiPolicies);
}
}
