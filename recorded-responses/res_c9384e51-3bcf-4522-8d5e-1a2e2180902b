(function ($) {
if(typeof $.fn.each2 == "undefined") {
$.extend($.fn, {
each2 : function (c) {
var j = $([0]), i = -1, l = this.length;
while (
++i < l
&& (j.context = j[0] = this[i])
&& c.call(j[0], i, j) !== false
);
return this;
}
});
}
})(jQuery);
